{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "RAILPACK",
    "installCommand": "pip install -r requirements.txt",
    "buildCommand": ""
  },
  "deploy": {
    "startCommand": "python3 main.py",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}