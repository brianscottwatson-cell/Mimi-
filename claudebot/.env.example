# Environment template
# Copy this to .env and fill in your values

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/claudebot

# Anthropic
ANTHROPIC_API_KEY=sk-ant-...

# Server
PORT=3000
NODE_ENV=development

# Optional: Voice — x.ai Eve (primary) or ElevenLabs (fallback)
# x.ai: Get key from https://console.x.ai — uses Eve voice via Realtime API
XAI_API_KEY=...
# XAI_VOICE=Eve          # Options: Ara, Rex, Sal, Eve, Leo

# ElevenLabs (fallback if x.ai not set): https://elevenlabs.io
ELEVENLABS_API_KEY=...

# Kimi K2.5 via NVIDIA NIM (cost-saving for lighter agent tasks)
# Get key from https://build.nvidia.com
NVIDIA_API_KEY=nvapi-...
# KIMI_MODEL=moonshotai/kimi-k2.5

# Telegram Bot (get token from @BotFather)
TELEGRAM_BOT_TOKEN=
# Optional: comma-separated Telegram user IDs to restrict access
# TELEGRAM_ALLOWED_USERS=123456789

# Optional: Code Execution Sandbox
# Set to true to enable /exec and /py commands
ENABLE_CODE_EXECUTION=true
