# Environment template
# Copy this to .env and fill in your values

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/claudebot

# Anthropic
ANTHROPIC_API_KEY=sk-ant-...

# Server
PORT=3000
NODE_ENV=development

# Optional: Voice (ElevenLabs)
# Get key from https://elevenlabs.io
ELEVENLABS_API_KEY=...

# Optional: Search (xAI Grok)
XAI_API_KEY=...

# Optional: Code Execution Sandbox
# Set to true to enable /exec and /py commands
ENABLE_CODE_EXECUTION=true
